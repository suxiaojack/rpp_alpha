//嵌套类和嵌套命名空间

namespace A
{
}

class A.B
{
	int m_b
	
	A.B()
	{
		printl('constructor')
	}

	static void func()
	{
		printl('func')
	}
}

namespace A.B.C
{
	void func()
	{
		printl('func')
	}
}

void main()
{
	A.B b
	b.m_b=3
	printl(b.m_b)
	A.B.func()
	A.B.C.func()
}